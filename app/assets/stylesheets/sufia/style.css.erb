a {
  color: #0082BF;
}

a:hover {
  color: #0082BF;
}

#wrapper {
  background: #d4d5d6 none repeat scroll 0 0 !important;
}

#masthead {
  min-height: 6em;
  background: white;
}

#NMLogo a .alt {
  display: none;
}

#NMLogo a {
  background: transparent url(<%= asset_data_uri('nm_nucats_cobrand_crop.png') %>) no-repeat scroll 0px 0px;
  background-size: auto 40px;
  display:block;
  float:left;
  height:50px;
  padding: 0px;
  width: 100%;
}

#nmRightLinks {
  margin-top: 15px;
}

#nmRightLinks a {
  font-family:Calibri,"Helvetica Neue",Helvetica,Arial,sans-serif;
  color: #7F8283;
  font-size: 16px;
}

#nmRightLinks a:hover {
  opacity: .75;
  text-decoration: none;
}

#feinbergTopLink, #feinbergBottomLink {
  margin-top: 10px;
  color: #FFF;
  font-size: 16px;
  text-decoration: none;
}

#digitalHubTopLink {
  color: #FFF;
  font-size: 40px;
  font-family: "Calibri Light", Calibri, Arial;
  text-decoration: none;
}

#topTitle {
  margin-bottom: 5px;
}

a#digitalHubTopLink:hover, a#feinbergTopLink:hover, a#NMLogo:hover {
  opacity: .75;
}

#betaLabel {
  top: -1.0em;
  font-size: 16px;
  font-family: "Calibri Light", Calibri, Arial;
  padding: 3px 6px;
  font-weight: 400;
}

#nmBottom {
  background-color: #514689;
}

#user_utility_xs #user_utility_links {
  margin-top: 0px;
  padding-bottom: 7px;
}

#user_utility_xs .login_button {
  margin-top: 0px;
  padding-bottom: 7px;
}

#home_header {
  background-image: url(<%= asset_data_uri('dh_cell_1170.jpg') %>);
  background-position: top;
  background-clip: border-box;
  border-top: 0px;
  border-bottom: 0px;
}

#home_content {
  padding-bottom: 0px;
  padding-top: 0px;
}

#homeTabs {
  padding-top: 7px;
  padding-left: 7px;
  background-color: #E6E6E6;
  font-family: Calibri,"Helvetica Neue",Helvetica,Arial,sans-serif;
  font-size: 16px;
  font-weight: bold;
  color: #54585A;
  border-bottom: 1px solid #514689;
}

#featured_container > h2 {
  display: none;
}

/* Featured works container */
#home_content > div {
  padding: 0px;
}

@media (max-width: 767px) {
  #home_content > div {
    border-right: none;
  }
}

@media (min-width: 768px) {
  #home_content > div:first-child {
    border-right: 6px solid #d4d5d6;
    border-left: none;
  }
}

#home_content > div > div:first-child {
  padding-bottom: 7px;
}

#home_content div h2 {
  margin-top: 0px;
  padding-top: 17px;
  padding-left: 7px;
}

#home_content > div > div h2 {
  font-family: Calibri,"Helvetica Neue",Helvetica,Arial,sans-serif;
}

/* Tag cloud container */
@media (min-width: 768px) {
  #home_content > div:nth-child(2) {
    border-left: 6px solid #d4d5d6;
    margin-left: -6px;
  }

  #home_content > div:nth-child(2) > div:first-child {
    margin-right: -6px;
  }
}

#home_content > div:nth-child(2) {
  padding: 0px;
}

#home_content > div:nth-child(2) > div:nth-child(1) h2 {
  font-family: Calibri,"Helvetica Neue",Helvetica,Arial,sans-serif;
  font-size: 16px;
  font-weight: 700;
  border-bottom: 1px solid #514689;
  background-color: #E6E6E6;
  color: #54585A;
  width: 100%;
}

#home_content table {
  background-color: white;
}

#home_content .tab-content {
  padding-left: 1px;
  padding-right: 1px;
}

/* Explore Tag Cloud*/
#home_content > div:nth-child(2) > div:nth-child(2) > div.row:first-child {
  margin-left: -7px;
  margin-right: 0px;
  border-bottom: 1px solid #514689;
  background-color: #E6E6E6;
  font-family: Calibri,"Helvetica Neue",Helvetica,Arial,sans-serif;
  color: #54585A;
}

#tag-cloud {
  padding-left: 8px;
  padding-right: 15px;
  padding-top: 7px; 
}

a.tag-toggle-list {
  font-family: Calibri,"Helvetica Neue",Helvetica,Arial,sans-serif;
  margin-top: -8px;
  margin-bottom: 8px;
  margin-right: 7px;
  float: right;
}

ul.tagcloud {
  padding: 15px;
}

ul.tagcloud a {
  color: #0082BF !important;
}

#masthead_controls {
  border-top: none;
  border-bottom: none;
  background-color: #E6E6E6;
}

#masthead_controls .navbar {
  background-color: #E6E6E6;
  margin-bottom: 0px;
  font-family: Calibri,"Helvetica Neue",Helvetica,Arial,sans-serif;
  font-weight: 400;
  font-size: 20px;
}

#masthead_controls .navbar li:first-child {
  border-left: 2px solid #FFF;
}

#masthead_controls .navbar li {
  border-right: 2px solid #FFF;
}

#masthead_controls .navbar a:link, #masthead_controls .navbar a:visited {
  color: #54585A;
  background-color: #E6E6E6;
}

#masthead_controls .navbar a:hover {
  background-color: #D4D5D6;
}

#masthead_controls .navbar-default .navbar-nav > .active > a, #masthead_controls .navbar-default .navbar-nav > .active > a:hover, #masthead_controls .navbar-default .navbar-nav > .active > a:focus {
  background-color: #FFF;
  color: #54585A;
  border-top: 3px solid #B1ACCE;
  border-bottom: 2px solid white;
  margin-top: -4px;
  margin-left: -2px;
  margin-right: -2px;
  padding-bottom: 17px;
}

#masthead_controls .navbar-default .navbar-nav > :not(.active) > a {
  padding-bottom: 18px;
}


@media (min-width: 768px) and (max-width: 1199px) {
  #masthead_controls > div:nth-child(1) {
    border-bottom: 1px solid white;
  }
}

.dashboard .panel-default > .dashboard-panel {
  //background-color: rgb(104, 72, 113);
  background-color: rgb(245,245,245);
  color: #333;
}

div.heading-tile .glyphicon, div.heading-tile .help-icon {
    background: none repeat scroll 0% 0% #FFAE00;
}

.dashboard .badge {
    background-color: #FFAE00;
}

div#announcement {
  background-color: white;
  color: black;
}

select[name="generic_file[resource_type][]"],
select[name="collection[resource_type][]"] {
  height: 12em;
}

.collection_description label, .genericfile_description label {
  font-family: Verdana, Arial, sans-serif;
  font-size: 13px;
  letter-spacing: 0px !important;
}

#collection_description, #genericfile_description {
  color: black;
}

/* NM Footer styles */
#footer, #brandFooterNew {
  background:#514689;
}

#footer {
  margin-top: 0px;
  padding: 0px;
}

#brandFooterNew {
  color:#fff;
  font-family:Calibri,"Trebuchet MS",Helvetica,Arial,sans-serif;
  padding-top: 2.16667%;
  letter-spacing:.4px
}

#brandFooterNew a {
  color:#fff;
  text-decoration:underline
}

#brandFooterNew a:hover {
  opacity:0.8
}

#brandFooterNew ul {
  list-style:none;
  padding:0
}

#brandFooterNew ul li {
  margin:10px 0
}

#brandFooterNew ul li a {
  padding:7px 0
}

#brandFooterNew p {
  color:#fff;
  line-height:1.8em;
  margin:1em 0
}

#brandFooterNew h2 a,#brandFooterNew h2 a:link {
  font-size:1em
}

#brandFooterNew #nuFooterNew,#brandFooterNew #footerLinks,#brandFooterNew #deptInfo,#brandFooterNew #footerMisc {
  color:#fff;
  line-height:1.8em;
  word-wrap:break-word;
  margin-bottom:1em
}

#brandFooterNew #nuFooterNew a,
#brandFooterNew #footerLinks a,
#brandFooterNew #deptInfo a,
#brandFooterNew #footerMisc a {
  color:#fff
}

#brandFooterNew #nuBrand img {
  width: 170px;
}

#brandFooterNew #deptInfo {
  line-height:1.3em;
  margin-left:1.5em
}
#brandFooterNew #deptInfo li.fa, #brandFooterNew #deptInfo dt.fa {
  float:left;
  margin-top:0;
  margin-left:-1.5em
}

#brandFooterNew #deptInfo .fa-map-marker {
  font-size:1.5em
}

#brandFooterNew #deptInfo .fa-mobile {
  font-size:2em
}

#brandFooterNew #footerMisc h2 {
  font-size:1em;
  font-weight:normal;
  line-height:1.3em
}

#brandFooterNew #footerMisc .social-icons li {
  float:left
}

#brandFooterNew #footerMisc .social-icons .fa {
  color:#514689;
  background:#fff;
  font-size:36px;
  width:40px;
  height:40px;
  line-height:40px;
  text-align:center;
  margin-right:.5em;
  padding:0;
  text-decoration:none
}

#brandFooterNew #footerMisc .social-icons .fa.fa-facebook:hover {
  background-color:#004088
}

#brandFooterNew #footerMisc .social-icons .fa.fa-twitter:hover {
  background-color:#00abe3
}

#brandFooterNew #footerMisc .social-icons .fa.fa-linkedin:hover {
  background-color:#01669c
}

#brandFooterNew #footerMisc .social-icons .fa.fa-flickr:hover {
  background-color:#FF0084
}

#brandFooterNew #footerMisc .social-icons .fa.fa-youtube:hover {
  background-color:#CC181E
}

#brandFooterNew #footerMisc .social-icons .fa.fa-instagram:hover {
  background-color:#517FA6
}

#brandFooterNew #footerMisc .social-icons .icon {
  transition:all .4s ease
}

#brandFooterNew #footerMisc .social-icons .icon:hover {
  color:#fff;
  opacity:1
}

// NM Social Media
a.Facebook,a.Twitter,a.Flickr,a.LinkedIn {
  background:url(http://deptcommon.fsm.northwestern.edu/images/nm/NM-FSM-sprite.png) no-repeat scroll 0 -250px transparent;
  display:block;
  float:left;
  font-size:0px;
  height:25px;
  margin-right:18px;
  width:25px;
  opacity:.7;
  filter:alpha(opacity=70)
}

a.Facebook:hover,a.Twitter:hover,a.Flickr:hover,a.LinkedIn:hover {
  opacity:1
}

a.Twitter {
  background-position:-28px -250px
}

a.Flickr {
  background-position:-55px -250px
}

a.LinkedIn {
  background-position:-82px -250px
}
/* End NM styles */

